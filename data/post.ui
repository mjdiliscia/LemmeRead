<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.90.2 -->
<interface>
  <!-- interface-name post.ui -->
  <requires lib="gtk" version="4.0"/>
  <requires lib="libadwaita" version="1.0" />
  <object class="GtkWindow">
    <property name="child">
      <object class="GtkBox" id="post">
        <property name="orientation">vertical</property>
        <child>
          <object class="AdwClamp" id="card">
            <property name="name">card</property>
            <property name="vexpand">1</property>
            <property name="visible">True</property>
            <property name="maximum_size">600</property>
            <property name="tightening_threshold">600</property>
            <child>
            <object class="GtkBox">
            <property name="orientation">vertical</property>
            <property name="spacing">5</property>
            <child>
              <object class="GtkLabel" id="title">
                <property name="label" translatable="yes">Title of post</property>
                <property name="max-width-chars">1</property>
                <property name="name">postTitle</property>
                <property name="wrap">1</property>
                <property name="xalign">0</property>
                <style>
                  <class name="content"/>
                </style>
              </object>
            </child>
            <child>
              <object class="GtkBox" id="subtitle">
                <property name="spacing">5</property>
                <child>
                  <object class="GtkImage" id="communityIcon"/>
                </child>
                <child>
                  <object class="GtkSeparator"/>
                </child>
                <child>
                  <object class="GtkLabel" id="communityName">
                    <property name="label" translatable="yes">Community name</property>
                    <property name="name">communityName</property>
                    <style>
                      <class name="content"/>
                    </style>
                  </object>
                </child>
                <child>
                  <object class="GtkSeparator"/>
                </child>
                <child>
                  <object class="GtkLabel" id="username">
                    <property name="label" translatable="yes">Username</property>
                    <property name="name">username</property>
                  </object>
                </child>
                <child>
                  <object class="GtkSeparator"/>
                </child>
                <child>
                  <object class="GtkLabel" id="time">
                    <property name="label" translatable="yes">Time</property>
                    <style>
                      <class name="content"/>
                    </style>
                  </object>
                </child>
                <child>
                  <object class="GtkLinkButton" id="linkButton">
                    <property name="focusable">1</property>
                    <property name="label" translatable="yes">Link</property>
                    <property name="receives-default">1</property>
                    <property name="visible">0</property>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkPicture" id="image">
                <property name="can-shrink">false</property>
                <property name="content-fit">contain</property>
                <property name="halign">center</property>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="description">
                <property name="label" translatable="yes">
</property>
                <property name="max-width-chars">1</property>
                <property name="selectable">1</property>
                <property name="use-markup">1</property>
                <property name="wrap">1</property>
                <property name="xalign">0</property>
                <style>
                  <class name="content"/>
                </style>
              </object>
            </child>
            <child>
              <object class="GtkBox">
                <child>
                  <object class="GtkSpinButton" id="votes">
                    <property name="focusable">1</property>
                    <property name="numeric">1</property>
                  </object>
                </child>
                <child>
                  <object class="GtkButton" id="commentsButton">
                    <property name="focusable">1</property>
                    <property name="label" translatable="yes">comments</property>
                    <property name="receives-default">1</property>
                    <property name="halign">right</property>
                  </object>
                </child>
              </object>
            </child>
            </object>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkBox" id="commentsParent">
            <property name="orientation">vertical</property>
            <property name="vexpand">1</property>
          </object>
        </child>
      </object>
    </property>
  </object>
</interface>
