<interface>
  <!-- interface-name mainWindow.ui -->
  <requires lib="gtk" version="4.0"/>
  <object class="AdwApplicationWindow" id="window">
    <property name="height-request">600</property>
    <property name="width-request">620</property>
    <property name="default-height">600</property>
    <property name="default-width">825</property>
    <child>
      <object class="AdwBreakpoint">
        <condition>max-width: 820sp</condition>
        <setter object="splitView" property="collapsed">True</setter>
      </object>
    </child>
    <property name="content">
      <object class="AdwNavigationSplitView" id="splitView">
